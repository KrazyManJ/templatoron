   <string notr="true">#Window {
	background: #292929;
}
#TemplateListFrame {
	background: #1F1F1F;
	min-width: 300px;
	border-right: 1px solid #555;
}
#TemplateLabel {
	color: white;
	font: 25pt &quot;Inter&quot;;
}
#BottomFrame {
	max-height: 150px;
}
#OutputPathFrame {
	background: #141414;
	max-height: 40px;
	min-height: 40px;
	border-radius: 10px;
	border: 1px solid #555;
}
QLineEdit {
	background: transparent;
	border: none;
	font: 10pt &quot;Fira Code&quot;;
	color: #ccc;
}
#OutputPathButton {
	border-image: url(:/content/folder-open.svg)
}
#ButtonsFrame QCheckBox {
	color: white;
	font: 8pt &quot;Inter&quot;;
}
#ButtonsFrame QPushButton {
	padding: 15px;
	font: 15pt &quot;Inter&quot;;
	background: #575757;
	border-radius: 10px;
	color: white;
}
#ButtonsFrame QPushButton:hover {
	background: #666;
}
#ButtonsFrame QPushButton:pressed {
	background: #777;
}
#TemplateListView{
	background: transparent;
	color: white;
	font: 12pt &quot;Inter&quot;;
	outline: 0;
}
#TemplateListView::item {
	padding: 2px;
	border-radius: 5px;
	color: white;
	margin-right: 10px;
}
#TemplateListView::item:selected {
	background: #222;
	border : 1px solid #666;
	color: white;
}
#TemplateListView::item:selected &gt; * {
    background-color: transparent;
}
#TemplateListView::item:selected QIcon {
    background-color: transparent;
    border: none;
}
QScrollBar:vertical{
	width: 20px;
	right: 20px;
	margin: 0px 10px 0px 0;
	background: transparent;
	border-radius: 5px;
}
QScrollBar::handle:vertical {
	background: #505050;
	border-radius: 5px;
}
QScrollBar::handle:vertical:hover {
	background: #5A5A5A;
}
QScrollBar::handle:vertical:pressed {
	background: #6A6A6A;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical
{
      border: none;
      background: none;
      color: none;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{
	background: #151515;
	border-radius:5px;
}
#DirectoryDisplay {
	font: 10pt &quot;Fira Code&quot;;
	background: #1c1c1c;
	border : 1px solid #666;
	border-radius: 20px;
	padding: 20px 0;
	outline: 0;
}
#DirectoryDisplay::item {
	padding: 2px;
	border-radius: 5px;
	color: #ccc;
	margin-right: 10px;
}
#DirectoryDisplay::item:selected {
	background: #222;
	border : 1px solid #666;
	color: white;
}
#DirectoryDisplay::branch {
	color: #ccc;
}
QLabel {
	color: #bbb;
	font: 8pt &quot;Inter&quot;;
}
#VariableList {
	background: transparent;
	border: none;
}
#VariableListContent {
	background: transparent;
	border: none;
}
#DirectoryDisplayLabel, #VariableListLabel{
	font: 13pt &quot;Inter&quot;;
	color: #aaa;
}</string>
